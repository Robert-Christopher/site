;
; Namespace programs and queues by project name.
;

[program:NAME_irc_bot]
command=cd PROJECT/bin && ./deta.sh li3.sh bot
numprocs=1
process_name=%(program_name)s_%(process_num)02d
redirect_stderr=true
stdout_logfile=PROJECT/log/%(program_name)s_%(process_num)02d.log
; stdout_logfile=NONE
; stdout_logfile_maxbytes=0
directory=PROJECT
user=USER

